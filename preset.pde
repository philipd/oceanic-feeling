class Preset
{
  public int iOffsetA;
  public int iOffsetB;
  public int iOffsetC;
  public int iShift;
  public int iOffsetD;
  public int iBlackness;
  
  public Preset(int iOffsetA, int iOffsetB, int iOffsetC, int iShift, int iOffsetD, int iBlackness)
  {
    this.iOffsetA = iOffsetA;
    this.iOffsetB = iOffsetB;
    this.iOffsetC = iOffsetC;
    this.iShift = iShift;
    this.iOffsetD = iOffsetD;
    this.iBlackness = iBlackness;
  }
}
